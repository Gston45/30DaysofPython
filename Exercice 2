#addition
a = 3 + 4
print(a)

#soustraction
b = 3 - 4
print(b)

#multiplication
c = 3 * 4
print(c)

#modulo
d = 3%4
print(d)

#division
e = 3 / 4
print(e)

#exponantielle
f = 3 ** 4
print(f)

#floor division operator
g = 3 // 4
print(g)

